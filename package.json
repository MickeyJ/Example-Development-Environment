{
  "name": "Example-Development-Environment",
  "version": "1.0.0",
  "description": "development environment tooling",
  "scripts": {
    "lint": "./node_modules/.bin/eslint .",
    "test": "mocha --compilers js:babel-core/register --require ./src/test/test_helper.js --recursive ./src/test",
    "test:watch": "npm run test -- --watch",
    "check": "gulp check",
    "check:watch": "gulp check-watch",
    "serve": "NODE_ENV=development webpack-dev-server",
    "clean": "rimraf dist",
    "build": "NODE_ENV=production npm run clean && webpack"
  },
  "keywords": [
    "eslint",
    "flow",
    "react",
    "redux",
    "webpack"
  ],
  "author": "Mickey J Malotte",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.26.1",
    "cssnano": "^3.10.0",
    "empty": "^0.10.1",
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "flow-bin": "^0.38.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-flowtype": "^1.0.0",
    "html-webpack-plugin": "^2.26.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "open-browser-webpack-plugin": "0.0.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "rimraf": "^2.5.4",
